{
  "name": "@ensdomains/ens-contracts",
  "version": "1.5.1",
  "description": "ENS contracts",
  "type": "module",
  "scripts": {
    "compile": "hardhat compile && bun scripts/generate-artifacts.ts",
    "test": "NODE_OPTIONS=\"--disable-warning=ExperimentalWarning\" vitest",
    "test:remote": "FORKING_ENABLED=1 bun run test ./test/**/Test*.remote.ts",
    "test:parallel": "bun run test ./test/**/Test*.ts --parallel",
    "test:local": "bun run test --network localhost",
    "test:deploy": "bun ./scripts/deploy-test.ts",
    "lint": "hardhat check",
    "build": "rm -rf ./build/deploy ./build/hardhat.config.js && hardhat compile && tsc",
    "format": "prettier --write .",
    "prepublishOnly": "bun run build",
    "pub": "npm publish --access public",
    "wikiCheck": "bun ./scripts/wikiCheck.ts",
    "generate-artifacts": "bun scripts/generate-artifacts.ts"
  },
  "files": [
    "build",
    "contracts/**/*.sol",
    "artifacts",
    "deployments"
  ],
  "main": "index.js",
  "devDependencies": {
    "@ensdomains/dnsprovejs": "^0.5.1",
    "@ensdomains/hardhat-chai-matchers-viem": "^0.1.8",
    "@nomicfoundation/hardhat-keystore": "^3.0.0-next.8",
    "@nomicfoundation/hardhat-network-helpers": "3.0.0-next.8",
    "@nomicfoundation/hardhat-viem": "3.0.0-next.8",
    "@rocketh/deploy": "^0.11.18",
    "@rocketh/read-execute": "^0.11.19",
    "@rocketh/verifier": "^0.11.18",
    "@types/node": "^18.0.0",
    "@viem/anvil": "^0.0.10",
    "abitype": "^1.0.2",
    "chai": "^5.1.1",
    "dotenv": "^16.4.5",
    "hardhat": "3.0.0-next.8",
    "hardhat-abi-exporter": "^2.9.0",
    "hardhat-contract-sizer": "^2.6.1",
    "hardhat-deploy": "^2.0.0-next.25",
    "hardhat-gas-reporter": "^1.0.4",
    "husky": "^9.1.7",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.24",
    "rocketh": "^0.11.20",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "viem": "2.28.1",
    "vitest": "^3.2.3"
  },
  "dependencies": {
    "@ensdomains/buffer": "^0.1.1",
    "@ensdomains/solsha1": "0.0.3",
    "@openzeppelin/contracts": "^4.1.0",
    "dns-packet": "^5.3.0"
  },
  "patchedDependencies": {
    "@nomicfoundation/hardhat-viem@3.0.0-next.8": "patches/@nomicfoundation%2Fhardhat-viem@3.0.0-next.8.patch",
    "rocketh@0.11.20": "patches/rocketh@0.11.20.patch"
  },
  "resolutions": {
    "@nomicfoundation/hardhat-viem": "3.0.0-next.8",
    "hardhat": "3.0.0-next.8"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ensdomains/ens-contracts.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ensdomains/ens-contracts/issues"
  },
  "homepage": "https://github.com/ensdomains/ens-contracts#readme"
}
